<Window x:Class="NotesTaker.CurrentPasswords"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="CurrentPasswords" Height="561" Width="492" Background="Chartreuse">
    <Grid Margin="0,0,-250,0">
        <Grid.ColumnDefinitions>
            <ColumnDefinition Width="198*"/>
            <ColumnDefinition Width="467*"/>
            <ColumnDefinition Width="81*"/>
        </Grid.ColumnDefinitions>
        <ListBox Name="usernames" HorizontalAlignment="Left" Height="511" Margin="7,5,0,0" VerticalAlignment="Top" Width="230" SelectionChanged="ListBox_SelectionChanged_1" Grid.ColumnSpan="2"/>
        <ListBox Name="passwords" HorizontalAlignment="Left" Height="511" Margin="46,5,0,0" VerticalAlignment="Top" Width="231" Grid.Column="1"/>
    </Grid>
</Window>
